{
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}/src",
    },
    "python.analysis.typeCheckingMode": "strict",
    "cmake.sourceDirectory": "${workspaceFolder}/traffic-monitor",
    "cmake.buildDirectory": "${workspaceFolder}/traffic-monitor/cmake-build-files",
    "cmake.debugConfig": {
        "stopAtEntry": false,
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Ignore SIGUSR1",
                "text": "handle SIGUSR1 nostop noprint"
            },
            {
                "description": "Ignore SIGUSR2",
                "text": "handle SIGUSR2 nostop noprint"
            }
        ]
    },
    "C_Cpp.default.compilerPath": "/usr/bin/gcc",
    "C_Cpp.errorSquiggles": "enabled",
    "files.associations": {
        "*.h": "cpp",
        "${workspaceFolder}/traffic-monitor/src/common/*.h": "c",
        "${workspaceFolder}/traffic-monitor/src/user/*.h": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "map": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "random": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "typeinfo": "cpp",
        "mutex": "cpp",
        "condition_variable": "cpp",
        "ctime": "cpp",
        "set": "cpp",
        "ratio": "cpp",
        "semaphore": "cpp",
        "stop_token": "cpp",
        "thread": "cpp",
        "test": "c",
        "*.ctemplate": "cpp",
        "unordered_set": "cpp"
    }
}